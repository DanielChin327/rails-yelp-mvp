<h1><%= @restaurant.name%></h1>
<p>Address: <%= @restaurant.address %></p>
<p>Phone: <%= @restaurant.phone_number %></p>
<p>Category: <%= @restaurant.category %></p>


<h2><%@restaurant.reviews.each do |review| %></h2>
        <%=review.content%>
        <%=review.rating%>
      <%end%>

<%=link_to "Go Back", restaurants_path%>
<%=link_to "Write a Review", reviews_path%>
